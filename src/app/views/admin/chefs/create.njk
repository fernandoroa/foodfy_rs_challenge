{% extends "../layout.njk" %}
{% block content %}
  <div class="admin_content">
    <div class="chef" id="{{ chef.id }}">
      <div class="same-line">
        <h1>Cadastrando Chef</h1>
        <button type="submit" class="right" form="form-create">Salvar Chef</button>
      </div>
      <form method="POST" action="/admin/chefs" enctype="multipart/form-data" id="form-create">
        {% include 'admin/chefs/fields.njk' %}
      </form>
    </div>
  </div>
{% endblock %}
{% block js %}
  <script type="module">
    import {ChefPhotoUpload} from '/ChefPhotoUpload.js'
    window.ChefPhotoUpload = ChefPhotoUpload;
  </script>
{% endblock %}